{
  "name": "sse-gen",
  "version": "1.0.0",
  "description": "Server-Sent Event client using Generators",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.cjs.js",
    "dist/index.esm.js",
    "dist/index.d.ts"
  ],
  "scripts": {
    "prepare": "husky install",
    "type:check": "tsc --noEmit",
    "lint": "eslint {src,test}/*",
    "lint:fix": "eslint --fix {src,test}/*",
    "format": "prettier --write --ignore-unknown {src,test}/*",
    "format:check": "prettier --check {src,test}/*",
    "test": "vitest run --coverage",
    "pre-commit": "lint-staged",
    "prebuild": "rimraf dist && mkdir dist",
    "build": "npm run build:types && npm run build:lib",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:lib": "rollup -c",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "sse",
    "server-sent",
    "events",
    "client",
    "generator"
  ],
  "author": "Antonio Villagra De La Cruz",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntonioVdlC/sse-gen.git"
  },
  "bugs": {
    "url": "https://github.com/AntonioVdlC/sse-gen/issues"
  },
  "homepage": "https://github.com/AntonioVdlC/sse-gen#readme",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.1",
    "rimraf": "^5.0.5",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  }
}